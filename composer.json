{
    "name": "survos/inspection-bundle",
    "description": "Inspects Doctrine entities with APIPlatform attributes",
    "license": "MIT",
    "authors": [
        {
            "name": "Tac Tacelosky",
            "email": "tacman@gmail.com"
        }
    ],
    "type": "symfony-bundle",
    "require": {
        "php": "^8.4",
        "survos/core-bundle": "^2.0",
        "symfony/config": "^7.4||^8.0",
        "symfony/dependency-injection": "^7.4||^8.0",
        "symfony/http-kernel": "^7.4||^8.0",
        "twig/twig": "^3.4",
        "symfony/string": "^7.4||^8.0",
        "symfony/framework-bundle": "^7.4||^8.0"
    },
    "require-dev": {
        "api-platform/core": "^4.0",
        "phpstan/phpstan": "^2.0",
        "roave/security-advisories": "dev-latest",
        "symfony/routing": "^7.4||^8.0",
        "cebe/php-openapi": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Survos\\InspectionBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.1-dev"
        }
    }
}
